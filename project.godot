[application]

config/name="RoboTD"
run/main_scene="res://scenes/MainMenu.tscn"
boot_splash/image="res://tiles/icons/andorid_icon.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.12549, 0.156863, 0.25098, 1 )
config/icon="res://tiles/icons/andorid_icon.png"

[autoload]

ScreenUISingleton="*res://scripts/singletone/ScreenUISingleton.gd"
LevelProcessState="*res://scripts/singletone/LevelProcessState.gd"
LevelSettingsReader="*res://scripts/singletone/LevelSettingsReader.gd"
LevelManager="*res://scripts/singletone/LevelManager.gd"
GameSaveService="*res://scripts/singletone/GameSaveService.gd"
LevelMusicPlayer="*res://scripts/singletone/LevelMusicPlayer.gd"

[display]

window/size/width=320
window/size/height=640
window/handheld/orientation="portrait"
mouse_cursor/custom_image="res://tiles/UI/mouse-pointer.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray(  )

[global]

view=false
main=false

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_clear_color=Color( 0.160784, 0.101961, 0.443137, 1 )
environment/default_environment="res://default_env.tres"
